<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
      <!-- Bootstrap CSS File -->
  <link href="/midterm/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">

  <!-- Libraries CSS Files -->
  <link href="/midterm/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet">
  <link href="/midterm/lib/animate/animate.min.css" rel="stylesheet">
  <link href="/midterm/lib/ionicons/css/ionicons.min.css" rel="stylesheet">
  <link href="/midterm/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
  <link href="/midterm/lib/lightbox/css/lightbox.min.css" rel="stylesheet">

  <!-- Main Stylesheet File -->
  <link href="/midterm/css/style.css" rel="stylesheet">
    <title>MIDTERM</title>
</head>
<body>
      <!-- NAVBAR WITH DROPDOWN -->
      <nav class="navbar navbar-expand-sm navbar-light">
        <div class="container">
            <a class="navbar-brand" href="#">網路程式設計</a>
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" href="/ararat">Home</a>
                </li>
                <li class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown">TKU</a>
                    <div class="dropdown-menu">
                        <a href="/tku/nav.html" class="dropdown-item">Nav</a>
                        <a href="/ararat/course" class="dropdown-item">My Class</a>
                        <a href="/resume.html" class="dropdown-item">My Resume</a>
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown">DBDEMO</a>
                    <div class="dropdown-menu">
                        <a href="/explanation.html" class="dropdown-item">Project Explanation</a>
                        <a href="/ararat/midterm" class="dropdown-item">Midterm</a>
                        <a href="/ararat/project" class="dropdown-item">Ararat Project</a>
                    </div>
                </li>
                <li class="nav-item">
                    <a href="/ararat/course" class="nav-link">My Class</a>
                </li>
                <li class="nav-item">
                    <a href="/resume.html" class="nav-link">My Resume</a>
                </li>
            </ul>
        </div>
    </nav>

    <!--==========================
      Portfolio Section
    ============================-->
    <section id="portfolio"  class="section-bg" >
        <div class="container">
          <header class="section-header">
            <h3 class="section-title">MIDTERM</h3>
          </header>

          <div class="row">
            <div class="col-lg-12">
              <ul id="portfolio-flters">
                <li data-filter="*" class="filter-active">All</li>
                <li data-filter=".filter-app">TEACHER</li>
                <li data-filter=".filter-web">MYSELF</li>
              </ul>
            </div>
          </div>
  
          <div class="row portfolio-container">
            <% for(let i=0 ; i < data.length ; i++) { %>
              <div class="col-lg-4 col-md-6 portfolio-item <%= data[i].select %> wow fadeInUp">
                <div class="portfolio-wrap">
                  <figure>
                    <img src="/midterm/img/portfolio/<%= data[i].pic %>" class="img-fluid" alt="">
                    <a href="/midterm/img/portfolio/<%= data[i].pic %>" data-lightbox="portfolio" data-title="App 1" class="link-preview" title="Preview"><i class="ion ion-eye"></i></a>
                    <a href="<%= data[i].href %>" class="link-details" title="More Details"><i class="ion ion-android-open"></i></a>
                  </figure>
    
                  <div class="portfolio-info">
                    <h4><a href="#"><%= data[i].name %></a></h4>
                    <p><%= data[i].category %></p>
                  </div>
                </div>
              </div>
            <% } %>
          </div>
        
          </div>
      </section><!-- #portfolio -->



        <!-- JavaScript Libraries -->
  <script src="/midterm/lib/jquery/jquery.min.js"></script>
  <script src="/midterm/lib/jquery/jquery-migrate.min.js"></script>
  <script src="/midterm/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/midterm/lib/easing/easing.min.js"></script>
  <script src="/midterm/lib/superfish/hoverIntent.js"></script>
  <script src="/midterm/lib/superfish/superfish.min.js"></script>
  <script src="/midterm/lib/wow/wow.min.js"></script>
  <script src="/midterm/lib/waypoints/waypoints.min.js"></script>
  <script src="/midterm/lib/counterup/counterup.min.js"></script>
  <script src="/midterm/lib/owlcarousel/owl.carousel.min.js"></script>
  <script src="/midterm/lib/isotope/isotope.pkgd.min.js"></script>
  <script src="/midterm/lib/lightbox/js/lightbox.min.js"></script>
  <script src="/midterm/lib/touchSwipe/jquery.touchSwipe.min.js"></script>
  <!-- Contact Form JavaScript File -->
  <script src="/midterm/contactform/contactform.js"></script>

  <!-- Template Main Javascript File -->
  <script src="/midterm/js/main.js"></script>
  
</body>
</html>