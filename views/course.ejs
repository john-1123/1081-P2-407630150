                 </tr>
             </thead>
             <tbody>
                 <% for(let i=0; i < data.length ; i++) { %>
                    <tr>
                        <th><%= data[i].id %></th>
                        <th><%= data[i].MON %></th>
                        <th><%= data[i].TUE %></th>
                        <th><%= data[i].WED %></th>
                        <th><%= data[i].THU %></th>
